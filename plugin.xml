<?xml version='1.0' encoding='utf-8'?>
<plugin id="north-plugin-device-test" version="1.0.0" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>northPluginDeviceTest</name>
    <js-module name="northPluginDeviceTest" src="www/northPluginDeviceTest.js">
        <clobbers target="deviceTest" />
    </js-module>

    <platform name="ios">
		<config-file target="config.xml" parent="/*">
			<feature name="northPluginDeviceTest">
				<param name="ios-package" value="northPluginDeviceTest" />
			</feature>
		</config-file>
		<source-file src="src/ios/northPluginDeviceTest.swift" />
	</platform>
</plugin>
